{
    "name": "laravelsn/phone-normalizer",
    "description": "simple wayy to normalize phone number in laravel app",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Laravelsn\\PhoneNormalizer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "LaravelSn",
            "email": "laravelsenegal@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "orchestra/testbench": "^10.6",
        "pestphp/pest": "^4.1",
        "laravel/pint": "^1.25"
    },
    "autoload-dev": {
        "psr-4": {
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0||^12.0",
        "illuminate/database": "^11.0||^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelsn\\PhoneNormalizer\\Providers\\PhoneNormalizerServiceProvider"
            ],
            "aliases": {
                "PhoneNormalizer": "Laravelsn\\PhoneNormalizer\\Facades\\PhoneNormalizer"
            }
        }
    }
}
